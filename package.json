  "dependencies": {
    "@carbon/vue": "^3.0.10",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^13.0.0",
    "vite": "^4.4.8",
    "vue-eslint-parser": "^9.3.2"
  }
}
